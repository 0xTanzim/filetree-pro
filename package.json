{
  "name": "filetreeproai",
  "displayName": "FileTreeProAI",
  "description": "Premium smart file tree generator for VS Code & Cursor with AI-powered insights",
  "version": "0.1.0",
  "publisher": "filetreeproai",
  "engines": {
    "vscode": "^1.102.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filetreeproai/filetreeproai.git"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "file-tree",
    "project-explorer",
    "file-visualization",
    "ai",
    "copilot",
    "productivity"
  ],
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "filetreeproai.refresh",
        "title": "Refresh File Tree",
        "icon": "media/icon.png"
      },
      {
        "command": "filetreeproai.search",
        "title": "Search Files",
        "icon": "media/icon.png"
      },
      {
        "command": "filetreeproai.export",
        "title": "Export Tree",
        "icon": "media/icon.png"
      },
      {
        "command": "filetreeproai.analyze",
        "title": "Analyze Project",
        "icon": "media/icon.png"
      },
      {
        "command": "filetreeproai.openFile",
        "title": "Open File",
        "icon": "media/icon.png"
      },
      {
        "command": "filetreeproai.copyPath",
        "title": "Copy Path",
        "icon": "media/icon.png"
      },
      {
        "command": "filetreeproai.revealInExplorer",
        "title": "Reveal in Explorer",
        "icon": "media/icon.png"
      },
      {
        "command": "filetreeproai.generateFileTree",
        "title": "Generate File Tree",
        "icon": "media/icon.png"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "filetreeproai.generateFileTree",
          "when": "explorerResourceIsFolder",
          "group": "navigation@1"
        }
      ]
    },
    "configuration": {
      "title": "FileTreeProAI",
      "properties": {
        "filetreeproai.exclude": {
          "type": "array",
          "default": [
            "node_modules",
            "dist",
            "build",
            "out",
            ".git",
            ".venv",
            "venv",
            "env",
            ".env",
            "target",
            "bin",
            "obj",
            ".vs",
            ".idea",
            "*.pyc",
            "*.log",
            "*.tmp",
            "*.cache"
          ],
          "description": "Files and folders to exclude from the file tree"
        },
        "filetreeproai.useCopilot": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI features using GitHub Copilot (if installed)"
        },
        "filetreeproai.maxDepth": {
          "type": "number",
          "default": 10,
          "description": "Maximum depth for file tree expansion"
        },
        "filetreeproai.showFileSize": {
          "type": "boolean",
          "default": true,
          "description": "Show file sizes in the tree"
        },
        "filetreeproai.showFileDate": {
          "type": "boolean",
          "default": false,
          "description": "Show file modification dates in the tree"
        },
        "filetreeproai.enableSearch": {
          "type": "boolean",
          "default": true,
          "description": "Enable search functionality"
        },
        "filetreeproai.enableAnalytics": {
          "type": "boolean",
          "default": true,
          "description": "Enable project analytics dashboard"
        },
        "filetreeproai.showIcons": {
          "type": "boolean",
          "default": true,
          "description": "Show file and folder icons in the generated tree"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "package": "node scripts/package-extension-fixed.js",
    "package:vsce": "@vscode/vsce package",
    "publish": "@vscode/vsce publish"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vscode/vsce": "^3.6.0",
    "archiver": "^7.0.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "filesize": "^11.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}

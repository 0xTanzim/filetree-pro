{
  "name": "filetree-pro",
  "displayName": "FileTree Pro",
  "description": "A powerful file tree generator for VS Code and Cursor. Generate beautiful file trees in multiple formats with smart exclusions and custom configurations.",
  "version": "0.1.0",
  "icon": "media/icon.png",
  "publisher": "0xtanzim",
  "engines": {
    "vscode": "^1.102.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xTanzim/filetree-pro.git"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "file-tree",
    "project-explorer",
    "file-visualization",
    "productivity",
    "file tree",
    "project explorer",
    "folder graph",
    "visual map",
    "structure viewer",
    "file tree",
    "project explorer",
    "folder graph",
    "visual map",
    "structure viewer"
  ],
  "galleryBanner": {
    "color": "#1E1E1E",
    "theme": "dark"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "filetree-pro.refresh",
        "title": "Refresh File Tree",
        "icon": "media/icon.png"
      },
      {
        "command": "filetree-pro.search",
        "title": "Search Files",
        "icon": "media/icon.png"
      },
      {
        "command": "filetree-pro.export",
        "title": "Export Tree",
        "icon": "media/icon.png"
      },
      {
        "command": "filetree-pro.analyze",
        "title": "Analyze Project",
        "icon": "media/icon.png"
      },
      {
        "command": "filetree-pro.openFile",
        "title": "Open File",
        "icon": "media/icon.png"
      },
      {
        "command": "filetree-pro.copyPath",
        "title": "Copy Path",
        "icon": "media/icon.png"
      },
      {
        "command": "filetree-pro.revealInExplorer",
        "title": "Reveal in Explorer",
        "icon": "media/icon.png"
      },
      {
        "command": "filetree-pro.generateFileTree",
        "title": "Generate File Tree",
        "icon": "media/icon.png"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "filetree-pro.generateFileTree",
          "when": "explorerResourceIsFolder",
          "group": "navigation@1"
        }
      ]
    },
    "configuration": {
      "title": "FileTree Pro",
      "properties": {
        "filetree-pro.exclude": {
          "type": "array",
          "default": [
            "node_modules",
            "dist",
            "build",
            "out",
            ".git",
            ".venv",
            "venv",
            "env",
            ".env",
            "target",
            "bin",
            "obj",
            ".vs",
            ".idea",
            "*.pyc",
            "*.log",
            "*.tmp",
            "*.cache"
          ],
          "description": "Files and folders to exclude from the file tree"
        },
        "filetree-pro.useCopilot": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI features using GitHub Copilot (if installed)"
        },
        "filetree-pro.maxDepth": {
          "type": "number",
          "default": 10,
          "description": "Maximum depth for file tree expansion"
        },
        "filetree-pro.showFileSize": {
          "type": "boolean",
          "default": true,
          "description": "Show file sizes in the tree"
        },
        "filetree-pro.showFileDate": {
          "type": "boolean",
          "default": false,
          "description": "Show file modification dates in the tree"
        },
        "filetree-pro.enableSearch": {
          "type": "boolean",
          "default": true,
          "description": "Enable search functionality"
        },
        "filetree-pro.enableAnalytics": {
          "type": "boolean",
          "default": true,
          "description": "Enable project analytics dashboard"
        },
        "filetree-pro.showIcons": {
          "type": "boolean",
          "default": true,
          "description": "Show file and folder icons in the generated tree"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "compile:prod": "tsc -p ./ --sourceMap false",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "package": "node scripts/package-extension.js",
    "package:optimized": "npm run build && node scripts/package-extension-optimized.js",
    "package:vsce": "@vscode/vsce package",
    "publish": "@vscode/vsce publish",
    "clean": "rm -rf out dist temp_package *.vsix",
    "dev": "npm run build:dev && code --extensionDevelopmentPath=."
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vscode/vsce": "^3.6.0",
    "archiver": "^7.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.6.2",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "typescript": "5.8.3",
    "webpack": "^5.101.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "filesize": "^11.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
